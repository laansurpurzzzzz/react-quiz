{
  "name": "react_sandbox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "host": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "prod": "yarn && yarn test:all:fix && yarn build && yarn preview",
    "ts-check": "yarn tsc --noEmit",
    "test:all": "yarn ts-check && yarn test:eslint && yarn test:stylelint",
    "test:all:fix": "yarn ts-check && yarn test:eslint:fix && yarn test:prettier && yarn test:stylelint:fix",
    "test:eslint": "eslint '**/*.{ts,tsx}' --cache --cache-strategy content",
    "test:eslint:fix": "yarn test:eslint --fix",
    "test:stylelint": "stylelint '**/*.css' --aei --cache --cache-strategy content",
    "test:stylelint:fix": "yarn test:stylelint --fix",
    "test:prettier": "prettier --write '**/*.{ts,tsx,md,json,yml}' --cache --cache-strategy metadata src",
    "test:commit": "commitlint -e",
    "generate-sprite": "yarn sprite --group --root 'public/images/svg-sprite-assets' -o 'public/images/svg-sprites' -d '~/types/icon.ts'"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "5.29.2",
    "@tanstack/react-query-devtools": "^5.29.2",
    "@types/lodash-es": "^4.17.12",
    "@types/react-datepicker": "6.2.0",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "1.6.8",
    "class-variance-authority": "^0.7.0",
    "classcat": "5.0.5",
    "clsx": "^2.1.1",
    "dayjs": "1.11.10",
    "effector": "23.2.0",
    "effector-react": "23.2.0",
    "keen-slider": "6.8.6",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.395.0",
    "plural-ru": "^2.0.2",
    "react": "18.2.0",
    "react-datepicker": "6.9.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.51.3",
    "react-hot-toast": "2.4.1",
    "react-input-mask": "2.0.4",
    "react-otp-input": "^3.1.1",
    "react-router-dom": "6.22.3",
    "react-textarea-autosize": "^8.5.3",
    "shadcn-ui": "^0.8.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.2",
    "@commitlint/config-conventional": "19.2.2",
    "@tailwindcss/line-clamp": "0.4.4",
    "@tanstack/eslint-plugin-query": "5.28.11",
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@types/react-input-mask": "3.0.5",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "@vitejs/plugin-react-swc": "3.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "8.50.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-plugin-dirs": "0.0.4",
    "eslint-plugin-effector": "0.14.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.2.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.6",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-tailwindcss": "3.15.1",
    "eslint-plugin-typescript-sort-keys": "3.2.0",
    "eslint-plugin-unicorn": "52.0.0",
    "eslint-plugin-unused-imports": "3.1.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "stylelint": "16.3.1",
    "stylelint-config-clean-order": "5.4.2",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "36.0.0",
    "stylelint-order": "6.0.4",
    "tailwindcss": "^3.4.3",
    "typescript": "5.4.5",
    "vite": "5.2.10",
    "vite-plugin-svgr": "4.2.0",
    "vite-tsconfig-paths": "4.3.2"
  }
}
